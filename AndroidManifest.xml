<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="xxx.bmwcodeinterpreter" android:versionCode="1" android:versionName="1.0">
	<application android:label="@string/app_name">
		<activity
			android:name=".BmwCodeInterpreterActivity"
			android:label="@string/app_name"
			android:screenOrientation="landscape"
			>
